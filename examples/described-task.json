{
  "taskDefinitionArn": "TaskARN",
  "containerDefinitions": [
    {
      "essential": true,
      "image": "amazon/amazon-ecs-sample",
      "memory": 100,
      "name": "sample",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "family": "hello-world",
